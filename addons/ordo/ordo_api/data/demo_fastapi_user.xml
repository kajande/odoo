<odoo>
    <record
        id="my_demo_app_user"
        model="res.users"
        context="{'no_reset_password': True}"
    >
        <field name="name">My Demo Endpoint User</field>
        <field name="login">my_demo_app_user</field>
        <field name="groups_id" eval="[(6, 0, [])]" />
    </record>

    <record
        id="social_media_user"
        model="res.users"
        context="{'no_reset_password': True}"
    >
        <field name="name">Social Media Endpoints User</field>
        <field name="login">social_media_user</field>
        <field name="groups_id" eval="[(6, 0, [])]" />
    </record>
</odoo>
