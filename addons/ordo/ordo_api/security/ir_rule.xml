<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_partner_demo_rule" model="ir.rule">
        <field name="name">Partner Rule for Demo API</field>
        <field name="model_id" ref="base.model_res_partner"/>
        <field name="domain_force">[(1, '=', 1)]</field>
        <field name="groups" eval="[(4, ref('my_demo_app_group'))]"/>
    </record>

    <record id="res_partner_social_media_rule" model="ir.rule">
        <field name="name">Partner Rule for Social Media API</field>
        <field name="model_id" ref="base.model_res_partner"/>
        <field name="domain_force">[(1, '=', 1)]</field>
        <field name="groups" eval="[(4, ref('social_media_group'))]"/>
    </record>
</odoo>
